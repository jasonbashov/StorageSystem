@using AAS.Web.Areas.CompanyManagment.Models.Sale
@model ICollection<SoldStockViewModel>

@foreach (var soldStock in Model)
{
    <strong>Stock Name: </strong> @Html.DisplayFor(m => soldStock.Name)  @*@soldStock.Name*@
        <strong> | Single Unit Price: </strong> @Html.DisplayFor(m => soldStock.SingleUnitPrice) @*@soldStock.SingleUnitPrice*@
        <strong> | Quantity Sold: </strong>@soldStock.QuantitySold
        <strong> | Total: </strong>@Html.DisplayFor(m => soldStock.SumPrice)
        <br />
}
